{
  "name": "Startup Advisory Services - Test ICP",
  "icp_text": "Small startup advisory and consulting firms with founder and advisor contact information for business development and partnership opportunities.",
  "queries": [
    "startup advisory services founder contact",
    "business consulting firm advisor email"
  ],
  "specific_urls": ["https://www.founderadvisors.tech/"],
  "default_contact_linkedin": "https://www.linkedin.com/in/rogertoennis/",
  "required_fields": {
    "company": ["name", "industry", "sub_industry"],
    "contact": ["email", "job_title", "full_name"]
  },
  "optional_fields": {
    "company": ["hq_location", "revenue_range", "employee_count", "specialties", "founded_year", "company_type", "number_of_locations", "ownership_type"],
    "contact": ["phone", "linkedin_url", "decision_maker", "department", "seniority"]
  },
  "threshold": 0.3,
  "filtering_strict": false,
  "mode": "fast",
  "refresh_policy": {
    "revisit_after_days": 90,
    "failure_revisit_days": 7,
    "domain_serp_ttl_hours": 48,
    "crawl_ttl_days": 14,
    "enrich_ttl_days": 30
  },
  "concurrency": {
    "max_concurrent_requests": 3
  },
  "search": {
    "max_pages": 5,
    "max_results_per_query": 50
  },
  "testing": {
    "process_rejected": true,
    "process_low_crawl": true,
    "force_contact_discovery": true
  },
  "caps": {
    "company_usd": 0.02,
    "contact_usd": 0.03,
    "run_usd_max": 10.0,
    "max_domains_per_run": 20,
    "max_crawl_per_run": 20,
    "max_enrich_per_run": 20
  },
  "role_priority": {
    "founder": 1,
    "owner": 1,
    "ceo": 1,
    "president": 1,
    "advisor": 2,
    "partner": 2,
    "director": 3,
    "consultant": 4,
    "default": 99
  },
  "exports": {
    "enabled": true
  },
  "default_country": "US",
  "exclusion_criteria": {
    "ownership_types": [],
    "max_locations": 10000,
    "excluded_keywords": []
  },
  "targeting": {
    "location_types": ["headquarters", "single_location"],
    "company_sizes": ["small", "mid-market"],
    "ownership_models": ["private"],
    "geographic_focus": ["North America", "United States"]
  },
  "intent_config": {
    "purpose": "startup_advisory",
    "target_action": "business_development",
    "intent_signals": {
      "high_intent": [
        "startup advisory",
        "business consulting",
        "founder coaching",
        "mentoring services",
        "advisory board",
        "startup support",
        "entrepreneur guidance",
        "business development",
        "strategic planning",
        "coaching services"
      ],
      "medium_intent": [
        "business services",
        "consulting",
        "advisory services",
        "professional services",
        "business support",
        "strategic advice",
        "mentoring",
        "coaching"
      ],
      "low_intent": [
        "not accepting clients",
        "fully booked",
        "no availability"
      ]
    },
    "intent_sources": [
      "about_pages",
      "team_pages",
      "contact_pages",
      "services_pages"
    ],
    "scoring_rules": {
      "high_intent_weight": 0.8,
      "medium_intent_weight": 0.4,
      "low_intent_weight": -0.3,
      "base_score": 0.5,
      "location_bonus": 0.1,
      "size_bonus": 0.1,
      "leadership_bonus": 0.1
    }
  }
}